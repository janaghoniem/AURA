{
  "workflow_id": "web_download_001",
  "workflow_name": "Web File Download Workflow",
  "description": "Search Google for files, click download button, and verify download",
  "version": "1.0.0",
  "context": "web",
  "tasks": [
    {
      "task_id": "web_dl_001",
      "action": "open_browser",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "open_browser",
        "browser": "edge",
        "url": "https://www.google.com"
      },
      "priority": "high",
      "timeout": 30,
      "retry_count": 2
    },
    {
      "task_id": "web_dl_002",
      "action": "web_search",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "web_search",
        "search_query": "filetype:pdf sample resume",
        "search_engine": "google"
      },
      "priority": "high",
      "timeout": 30,
      "retry_count": 2
    },
    {
      "task_id": "web_dl_003",
      "action": "click_search_result",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "click_search_result",
        "result_index": 0
      },
      "priority": "normal",
      "timeout": 20,
      "retry_count": 3
    },
    {
      "task_id": "web_dl_004",
      "action": "wait_page_load",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "wait_page_load",
        "timeout": 5
      },
      "priority": "normal",
      "timeout": 10,
      "retry_count": 1
    },
    {
      "task_id": "web_dl_005",
      "action": "find_download_button",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "find_element",
        "element_type": "button",
        "search_text": ["Download", "Get File", "Save", "PDF"],
        "search_method": "text"
      },
      "priority": "high",
      "timeout": 20,
      "retry_count": 3
    },
    {
      "task_id": "web_dl_006",
      "action": "click_download",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "click_element",
        "element_selector": "a[href*='download']",
        "selector_type": "css",
        "wait_time": 10
      },
      "priority": "normal",
      "timeout": 25,
      "retry_count": 1
    },
    {
      "task_id": "web_dl_007",
      "action": "wait_download",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "wait_download",
        "max_wait_time": 30
      },
      "priority": "normal",
      "timeout": 40,
      "retry_count": 1
    },
    {
      "task_id": "web_dl_008",
      "action": "get_page_title",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "get_page_info",
        "info_type": "title"
      },
      "priority": "low",
      "timeout": 5,
      "retry_count": 1
    },
    {
      "task_id": "web_dl_009",
      "action": "get_current_url",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "get_page_info",
        "info_type": "url"
      },
      "priority": "low",
      "timeout": 5,
      "retry_count": 1
    },
    {
      "task_id": "web_dl_010",
      "action": "create_download_folder",
      "context": "system",
      "strategy": "system",
      "params": {
        "action_type": "create_folder",
        "folder_path": "C:\\Users\\Public\\Downloads\\WorkflowDownloads"
      },
      "priority": "normal",
      "timeout": 10,
      "retry_count": 2
    },
    {
      "task_id": "web_dl_011",
      "action": "save_download_info",
      "context": "system",
      "strategy": "system",
      "params": {
        "action_type": "save_file",
        "file_path": "C:\\Users\\Public\\Downloads\\WorkflowDownloads\\download_log.txt",
        "content": "Download completed at {timestamp}\nSource: {url}\nFile: {filename}"
      },
      "priority": "low",
      "timeout": 10,
      "retry_count": 2
    },
    {
      "task_id": "web_dl_012",
      "action": "verify_download",
      "context": "system",
      "strategy": "system",
      "params": {
        "action_type": "verify_file",
        "folder": "C:\\Users\\Public\\Downloads"
      },
      "priority": "normal",
      "timeout": 10,
      "retry_count": 2
    },
    {
      "task_id": "web_dl_013",
      "action": "close_browser",
      "context": "web",
      "strategy": "web",
      "params": {
        "action_type": "close_browser"
      },
      "priority": "low",
      "timeout": 5,
      "retry_count": 1
    },
    {
      "task_id": "web_dl_014",
      "action": "workflow_complete",
      "context": "system",
      "strategy": "system",
      "params": {
        "action_type": "log_completion",
        "workflow_id": "web_download_001",
        "status": "completed"
      },
      "priority": "low",
      "timeout": 5,
      "retry_count": 1
    }
  ],
  "metadata": {
    "created_at": "2025-11-04T10:00:00Z",
    "author": "Accessibility AI Team",
    "tags": ["web", "download", "automation", "file-management"],
    "notes": "Updated with improved element detection and JavaScript click fallbacks"
  }
}